<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        body{
            margin:0;
            background-color: rgb(177, 250, 250) ;
        }
        @keyframes carmove {
    from{
        left: 0;
    }

    to{
        left: 75%;
    }
}

@keyframes wheelmove {
    
    from{
        transform: rotate(0deg);
    }

    to{
        transform: rotate(360deg);
    }
}
        #car{
            animation: carmove 3s linear 0s;
            top: 170px;
            animation-fill-mode: forwards;
            /* ya property car ko stop karny k liye use hoti hy */
            position: absolute;
            width: 300px;

            #car-body{
                width: 100%; 
            }
        } 
     .car-wheel:nth-child(2){
        width: 50px;
        right: 37px;
        top: 67px;
        position: absolute;

}

.car-wheel:nth-child(3){
    width: 50px;
    top:65px ;
    left: 46px;
    position: absolute;
} 
.road{
    width: 100%;
    display: flex;
   padding-top: 130px;
 }
 .car-wheel{
    animation: wheelmove 0.5s linear 0s;
    animation-iteration-count: 6;
    width: 50px;
    height: 50px;
    position: absolute;
}
    </style>
</head>
<body>
    <div>
        <img class="road"   src="myroad.jpg" alt="">
    </div>
    <div id="car">
    <img  id="car-body" src="my-car.png" alt="">
    <img class="car-wheel"  src="wheel.png" alt="">
    <img class="car-wheel" src="wheel.png" alt="">
    </div>
</body>
</html>
